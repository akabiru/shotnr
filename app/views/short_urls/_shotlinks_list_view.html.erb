<li class="list-group-item">
  <span class="label label-success label-pill pull-xs-right"><%= pluralize(s.clicks, 'click') %></span>
  <%= link_to full_short_url(s.short_url), full_short_url(s.short_url), target: '_blank' %>
  <% if s.active? %>
    <span class="text-success"><i class="fa fa-check-circle-o" aria-hidden="true"></i> active</span>
  <% else %>
    <span class="text-danger"><i class="fa fa-times-circle" aria-hidden="true"></i> inactive</span>
  <% end %> <br>
  <small class="text-muted"><%= s.long_url %></small>
</li>
